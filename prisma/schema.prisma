generator client {
  provider = "prisma-client"
  output = "../generated/client"
  engineType = "client"
}


datasource db {
  provider = "postgresql"
}

model Riwayat {
  id              Int      @id @default(autoincrement())
  nama            String
  jenisCucian     String
  berat           Float
  tipePembayaran  String
  addOn           String
  createdAt       DateTime @default(now())
}
